<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[]"
        cq:template="/conf/wknd/settings/wcm/templates/experience-fragment-web-variation-template"
        cq:xfMasterVariation="{Boolean}true"
        cq:xfVariantType="web"
        jcr:primaryType="cq:PageContent"
        jcr:title="Footer"
        sling:resourceType="wknd/components/structure/xfpage">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
           <responsivegrid
               jcr:primaryType="nt:unstructured"
               sling:resourceType="wcm/foundation/components/responsivegrid">
               <cq:responsive jcr:primaryType="nt:unstructured"/>
               <navigation
                   jcr:primaryType="nt:unstructured"
                   sling:resourceType="wknd/components/structure/navigation"
                   accessibilityLabel="Footer navigation"
                   collectAllPages="false"
                   navigationRoot="/content/wknd/language-masters/en"
                   skipNavigationRoot="false"
                   structureDepth="1">
                   <cq:responsive jcr:primaryType="nt:unstructured">
                       <default
                           jcr:primaryType="nt:unstructured"
                           offset="0"
                           width="6"/>
                       <tablet
                           jcr:primaryType="nt:unstructured"
                           width="8"/>
                       <phone
                           jcr:primaryType="nt:unstructured"
                           width="8"/>
                   </cq:responsive>
               </navigation>
               <separator
                   jcr:primaryType="nt:unstructured"
                   sling:resourceType="wknd/components/content/separator"/>
               <text
                   jcr:primaryType="nt:unstructured"
                   sling:resourceType="wknd/components/content/text"
                   text="&lt;p>â’¸ 2019, WKND Site.&lt;/p>&#xd;&#xa;"
                   textIsRich="true"/>
           </responsivegrid>
        </root>
    </jcr:content>
</jcr:root>
